<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jdbcUrlTemplate>

    <product name="HSQLDB">
        <driver name="org.hsqldb.jdbcDriver">
            <template><![CDATA[jdbc:hsqldb:hsql://<server>[:<9001>]/<dbname>]]></template>
        </driver>
    </product>

    <product name="DB2">
        <driver name="COM.ibm.db2.jdbc.net.DB2Drive">
            <template><![CDATA[jdbc:derby:<dbname>[;create=true]]]></template>
        </driver>
        <driver name="com.ibm.db2.jcc.DB2Driver">
            <template><![CDATA[jdbc:db2//<host_name>:50000/<dbname>]]></template>
        </driver>
    </product>

    <product name="PostgreSQL">
        <driver name="org.postgresql.Driver">
            <template><![CDATA[jdbc:postgresql:[<//host>[:<5432>/]]<database>]]></template>
        </driver>
    </product>

    <product name="MySQL">
        <driver name="com.mysql.cj.jdbc.Driver">
            <template><![CDATA[jdbc:mysql://<hostname>[<:3306>]/<dbname>]]></template>
        </driver>
        <driver name="com.mysql.jdbc.Driver">
            <template><![CDATA[jdbc:mysql://<hostname>[<:3306>]/<dbname>]]></template>
        </driver>
        <driver name="org.gjt.mm.mysql.Driver">
            <template><![CDATA[jdbc:mysql://<hostname>[<:3306>]/<dbname>]]></template>
        </driver>
    </product>

    <product name="H2">
        <driver name="org.h2.Driver">
            <template><![CDATA[jdbc:h2:tcp://<server>:9092/<dbname>]]></template>
        </driver>
    </product>

    <product name="MSSQL">
        <driver name="com.microsoft.jdbc.sqlserver.SQLServerDriver">
            <template><![CDATA[jdbc:microsoft:sqlserver://<server_name>:1433;DatabaseName=<dbname>]]></template>
        </driver>
        <driver name="com.microsoft.sqlserver.jdbc.SQLServerDriver">
            <template><![CDATA[jdbc:sqlserver://<server_name>:1433;databaseName=<dbname>]]></template>
        </driver>
    </product>

    <product name="Oracle">
        <driver name="oracle.jdbc.driver.OracleDriver">
            <template><![CDATA[jdbc:oracle:thin:@<server>[:<1521>]:<database_name>]]></template>
        </driver>
    </product>

</jdbcUrlTemplate>
